<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="edu.samibialozynski2washcoll.thesis2018.MainActivity">


    <Button
        android:id="@+id/samiButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sami"
        />
    
    <Button
        android:id="@+id/courtButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/courtney"/>

    <Button
        android:id="@+id/lizaButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/liza"/>

    <Button
        android:id="@+id/elleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/elle"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/samiSchedule"/>

</LinearLayout>
